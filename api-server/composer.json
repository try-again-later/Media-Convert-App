{
    "name": "try-again-later/media-convert-app-api",
    "type": "project",
    "autoload": {
        "psr-4": {
            "TryAgainLater\\MediaConvertAppApi\\": "src/"
        }
    },
    "scripts": {
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "php -S localhost:8080 -c ./php.ini -t ./public"
        ],
        "migrate": "php -c ./php.ini ./src/migrate.php",
        "migrate:fresh": "php -c ./php.ini ./src/migrate.php fresh"
    },
    "require": {
        "aws/aws-sdk-php": "^3.233",
        "vlucas/phpdotenv": "^5.4",
        "react/zmq": "^0.4.0",
        "ramsey/uuid": "^4.4"
    }
}
